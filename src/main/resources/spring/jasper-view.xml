<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <!-- Cliente -->
    <bean id="clienteReport" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
        <property name="url" value="classpath:reports/ClienteReport.jrxml" />
        <property name="reportDataKey" value="datasource" />
    </bean>

</beans>